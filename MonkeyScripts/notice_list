rm -rf *
adb uninstall com.cootek.test
adb install -r /var/lib/jenkins/opt/data/cootekime/MonkeyScripts/AllEditTextTest.apk
/bin/bash +x /var/lib/jenkins/opt/data/cootekime/MonkeyScripts/monkeyjob.sh com.cootek.smartinputv5
cat output_monkey/mail_content


